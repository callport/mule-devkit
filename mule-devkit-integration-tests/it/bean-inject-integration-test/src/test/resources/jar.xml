<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:jar="http://www.mulesoft.org/schema/mule/jar"
      xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/3.1/mule.xsd
        http://www.mulesoft.org/schema/mule/jar http://www.mulesoft.org/schema/mule/jar/1.0/mule-jar.xsd">

	<spring:bean id="jarFile" class="java.util.jar.JarFile"></spring:bean>
	
	<spring:bean id="manifest" class="java.util.jar.Manifest"></spring:bean>	

    <jar:config jarFile-ref="jarFile"/>

    <flow name="setManifest">
        <jar:set-manifest manifest-ref="manifest"/>
    </flow>

</mule>
