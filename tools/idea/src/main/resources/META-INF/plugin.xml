<idea-plugin version="2">
    <name>Mule DevKit</name>
    <description>Development Kit for Mule</description>
    <version>1.0</version>
    <vendor>MuleSoft Inc.</vendor>
    <idea-version since-build="8000"/>

    <resource-bundle xmlns="">org.mule.devkit.idea.MuleDevKitBundle</resource-bundle>

    <application-components>
        <component>
            <implementation-class>org.mule.devkit.idea.MuleDevKitApplicationComponent</implementation-class>
            <interface-class>org.mule.devkit.idea.MuleDevKitApplicationComponent</interface-class>
        </component>
    </application-components>

    <project-components>
        <!-- Add your project components here -->
    </project-components>

    <actions>
        <!-- Add your actions here -->
    </actions>

    <extensions defaultExtensionNs="com.intellij">
        <errorHandler implementation="com.intellij.diagnostic.ITNReporter"/>
        <moduleType id="MULE_MODULE" implementationClass="org.mule.devkit.idea.module.MuleModuleType" classpathProvider="true"/>
        <iconProvider implementation="org.mule.devkit.idea.MuleDevKitIconProvider"/>
    </extensions>
</idea-plugin>